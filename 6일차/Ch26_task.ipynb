   "source": [
    "def add_numbers(start, end):\n",
    "    if start % 2 == 0:\n",
    "        start +=1\n",
    "    for num in range(start,  end + 1, 2):\n",
    "        yield num\n",
    "for number in add_numbers(1,10):\n",
    "    print(number)"
   ]
  }
